{
  "name": "vscode-pylonn",
  "displayName": "vscode-pylonn",
  "description": "gen dependence tree",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pylon.init",
    "onCommand:pylon.genDependentTree"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "pylon.init",
        "title": "Init Pylon"
      },
      {
        "command": "pylon.genDependentTree",
        "title": "Gen Dependent Tree"
      },
      {
        "command": "pylon.genBeDependentTree",
        "title": "Gen BeDependent Tree"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "pylon.genDependentTree",
          "when": "resourceLangId == javascriptreact",
          "group": "pylon@1"
        },
        {
          "command": "pylon.genDependentTree",
          "when": "resourceLangId == javascript",
          "group": "pylon@1"
        },
        {
          "command": "pylon.genDependentTree",
          "when": "resourceLangId == typescriptreact",
          "group": "pylon@1"
        },
        {
          "command": "pylon.genDependentTree",
          "when": "resourceLangId == typescript",
          "group": "pylon@1"
        },
        {
          "command": "pylon.genBeDependentTree",
          "when": "resourceLangId == javascriptreact",
          "group": "pylon@1"
        },
        {
          "command": "pylon.genBeDependentTree",
          "when": "resourceLangId == javascript",
          "group": "pylon@1"
        },
        {
          "command": "pylon.genBeDependentTree",
          "when": "resourceLangId == typescriptreact",
          "group": "pylon@1"
        },
        {
          "command": "pylon.genBeDependentTree",
          "when": "resourceLangId == typescript",
          "group": "pylon@1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
